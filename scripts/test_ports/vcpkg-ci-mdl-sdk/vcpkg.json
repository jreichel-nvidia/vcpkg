{
  "name": "vcpkg-ci-mdl-sdk",
  "version-string": "ci",
  "description": "Port to force features of certain ports within CI",
  "license": "BSD-3-Clause",
  "supports": "!x86 & !(windows & (staticcrt | arm | uwp | static)) & !(osx & arm) & !android",
  "dependencies": [
    {
      "name": "mdl-sdk",
      "default-features": false,
      "features": [
        "dds",
        "openimageio"
      ]
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ]
}
